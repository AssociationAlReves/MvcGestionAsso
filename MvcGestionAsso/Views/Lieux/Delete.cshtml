@model MvcGestionAsso.Models.Lieu

@{
    ViewBag.Title = "Supprimer";
}

<h2>@ViewBag.Title</h2>

<h3>Etes-vous sûr de vouloir supprimer ceci ?</h3>
<div>
    <h4>Lieu</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.LieuCode)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LieuCode)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.LieuNom)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LieuNom)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Adresse)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Adresse)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Adresse2)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Adresse2)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.CodePostal)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CodePostal)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Ville)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Ville)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Supprimer" class="btn btn-default" /> |
            @Html.ActionLink("Retour à la liste", "Index")
        </div>
    }
</div>
